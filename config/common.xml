
<translog>
	<analyser>
		<type>bandwidth</type>
		<pace>10</pace>
	</analyser>
	<analyser>
		<type>status</type>
		<pace>-1</pace>
	</analyser>
	<analyser>
		<type>status</type>
		<pace>10</pace>
	</analyser>
	<analyser>
		<type>xactrate</type>
		<pace>1</pace>
	</analyser>
	<analyser>
		<type>requestdes</type>
		<pace>10</pace>
	</analyser>
	<analyser>
		<type>requestdes</type>
		<pace>-1</pace>
	</analyser>
	<analyser>
		<type>consumed</type>
		<pace>10</pace>
	</analyser>
	<analyser>
		<type>tmpconn</type>
		<servTime>100</servTime>
		<pace>10</pace>
	</analyser>
	<analyser>
		<type>tmpconn</type>
		<client>all</client>
		<servTime>100</servTime>
		<pace>-1</pace>
	</analyser>
	<analyser>
		<type>assemble</type>
		<pace>-1</pace>
		<filters>
			<orExp>
				<filter>
					<fmtName>urlAll</fmtName>
					<andExp>
						<suffix>m3u8</suffix>
						<keyword>036</keyword>
					</andExp>
				</filter>
				<filter>
					<fmtName>status</fmtName>
					<low>304</low>
					<high>304</high>
				</filter>
			</orExp>
			<filter>
				<fmtName>description</fmtName>
				<orExp>
					<keyword>TCP_REFRESH_MISS</keyword>
					<length>
						<low>8</low>
						<high>8</high>
					</length>
				</orExp>
			</filter>
		</filters>
	</analyser>
	<analyser>
		<type>counter</type>
		<pace>1</pace>
		<filters>
			<orExp>
				<filter>
					<fmtName>urlAll</fmtName>
					<andExp>
						<suffix>m3u8</suffix>
						<keyword>036</keyword>
					</andExp>
				</filter>
				<filter>
					<fmtName>status</fmtName>
					<low>304</low>
					<high>304</high>
				</filter>
			</orExp>
			<filter>
				<fmtName>description</fmtName>
				<orExp>
					<keyword>TCP_REFRESH_MISS</keyword>
					<length>
						<low>8</low>
						<high>8</high>
					</length>
				</orExp>
			</filter>
		</filters>
	</analyser>
</translog>
