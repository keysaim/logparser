
<translog>
	<!--outPath>../hlstest/output/web</outPath-->
	<pace>10</pace>
	<formatter>
		<filters>
			<id>ft1</id>
			<filter>
				<fmtName>description</fmtName>
				<keyword>HIT</keyword>
			</filter>
		</filters>
		<field>
			<blockValue>
				<if con="@ft1">
					<value>CACHE_HIT</value>
				</if>
				<else>
					<value>CACHE_MISS</value>
				</else>
			</blockValue>
			<fmtName>cacheStatus</fmtName>
		</field>
	</formatter>
	<analyser>
		<type>xactrate</type>
		<outputs>
			<output>
				<fmtName>servTime</fmtName>
				<expType>average</expType>
			</output>
			<output>
				<fmtName>cacheStatus</fmtName>
				<expType>map</expType>
				<output>
					<fmtName>servTime</fmtName>
					<expType>average</expType>
				</output>
			</output>
		</outputs>
	</analyser>
	<analyser>
		<type>activeSessions</type>
	</analyser>
	<analyser>
		<type>bandwidth</type>
	</analyser>
	<analyser>
		<outputs>
			<output>
				<fmtName>status</fmtName>
				<expType>map</expType>
			</output>
		</outputs>
	</analyser>
</translog>
