
<translog>
	<pace>1</pace>
	<formatter>
		<strict>1</strict>
		<filters>
			<id>ft1</id>
			<filter>
				<fmtName>qualitylevels</fmtName>
				<high>800000</high>
			</filter>
		</filters>
		<filters>
			<id>ft2</id>
			<filter>
				<fmtName>qualitylevels</fmtName>
				<high>1200000</high>
			</filter>
		</filters>
		<field>
			<!--http://se-rm4-10.se.pctvvod.mediasetpremium.alice.cdn.interbusiness.it/asset04/2013/10/pctv_47579.ism/qualitylevels(1600000)/fragments(video=9840000000)-->
			<value>(%u)</value>
			<fieldFmt>
				<case>lower</case>
				<match>http://(\S+\.se\.)?(\S+?)/\S+/(\S+.ism.?)/qualitylevels\((\S+?)\)/fragment\w*\((\S+?)=(\S+?)\)</match>
				<filters>
					<id>ft2</id>
					<filter>
						<fmtName>qualitylevels</fmtName>
						<high>5200000</high>
					</filter>
				</filters>
				<field>
					<value>($1)</value>
					<fmtName>seName</fmtName>
				</field>
				<field>
					<value>($2)</value>
					<fmtName>hostName</fmtName>
				</field>
				<field>
					<value>($3)</value>
					<fmtName>ismFile</fmtName>
				</field>
				<field>
					<expValue>($4)/1</expValue>
					<fmtName>qualitylevels</fmtName>
					<type>int</type>
				</field>
				<field>
					<value>($5)</value>
					<fmtName>mediaType</fmtName>
				</field>
				<field>
					<value>($6)</value>
					<fmtName>fragSequence</fmtName>
				</field>

				<field>
					<filters>
						<id>ft2</id>
						<filter>
							<fmtName>qualitylevels</fmtName>
							<high>200000</high>
						</filter>
					</filters>
					<blockValue>
						<if con="@ft1">
							<expValue>(@qualitylevels)+2.1</expValue>
						</if>
						<elif con="(@qualitylevels)&lt;1000000">
							<expValue>(@qualitylevels)+3.2</expValue>
						</elif>
						<elif con="@ft2">
							<expValue>(@qualitylevels)+4.3</expValue>
						</elif>
						<else>
							<expValue>(@qualitylevels)+5.4</expValue>
						</else>
					</blockValue>
					<fmtName>qualitylevels</fmtName>
					<type>int</type>
				</field>
			</fieldFmt>
		</field>

	</formatter>

	<analyser>
		<outputs>
			<output>
				<fmtName>qualitylevels</fmtName>
				<expType>map</expType>
			</output>
			<output>
				<expType>count</expType>
				<timing>1</timing>
			</output>
			<!--output>
				<fmtName>servTime</fmtName>
				<expType>timeActive</expType>
				<timing>1</timing>
			</output-->
		</outputs>
	</analyser>
</translog>
