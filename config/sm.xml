
<translog>
	<formatter>
		<field>
			<expValue>(%Z)-3*3600</expValue>
			<fmtName>recvdTime</fmtName>
			<type>int</type>
		</field>
		<field>
			<value>(%sm_pdk)</value>
			<fieldFmt>
				<split>\|</split>
				<field>
					<value>($-1)</value>
					<fmtName>totalDisk</fmtName>
				</field>
				<field>
					<value>($-12)</value>
					<fmtName>diskUsage1</fmtName>
				</field>
			</fieldFmt>
		</field>
		<!--field>
			<value>(%sm_pdk)</value>
			<fieldFmt>
				<find>[^|]+</find>
				<field>
					<value>($-1)</value>
					<fmtName>totalDiskF</fmtName>
				</field>
				<field>
					<value>($-12)</value>
					<fmtName>diskUsage1F</fmtName>
				</field>
			</fieldFmt>
		</field-->
	</formatter>
	<analyser>
		<outputs>
			<output>
				<fmtName>sm_cpu</fmtName>
			</output>
			<output>
				<fmtName>sm_mem</fmtName>
			</output>
			<output>
				<fmtName>sm_kmem</fmtName>
			</output>
			<output>
				<fmtName>sm_disk</fmtName>
			</output>
			<output>
				<fmtName>sm_web_cpu</fmtName>
			</output>
			<output>
				<fmtName>sm_web_requests</fmtName>
			</output>
			<output>
				<fmtName>sm_web_sessions</fmtName>
			</output>
			<output>
				<fmtName>sm_web_upstreams</fmtName>
			</output>
			<output>
				<fmtName>totalDisk</fmtName>
			</output>
			<output>
				<fmtName>sm_perdisk</fmtName>
			</output>
		</outputs>
		<pace>2</pace>
	</analyser>
</translog>
