		</field>

		<field>
			<blockValue>
				<if con="'(@caseOwner)'=='hai rong'">
					<value>hairong</value>
				</if>
				<elif con="'(@caseOwner)'=='yorma'">
					<value>york</value>
				</elif>
				<elif con="'(@caseOwner)'=='wanxing'">
					<value>wanxiang</value>
				</elif>
				<else>
					<value>(@caseOwner)</value>
				</else>
			</blockValue>
			<fmtName>caseOwner</fmtName>
		</field>

		<field>
			<blockValue>
				<if con="'(@customer)'=='telecom argentina'">
					<value>ta</value>
				</if>
				<else>
					<value>(@customer)</value>
				</else>
			</blockValue>
			<fmtName>customer</fmtName>
		</field>

		<field>
			<blockValue>
				<if con="len('($21)')&gt;8">
					<value>1</value>
				</if>
				<else>
					<value>0</value>
				</else>
			</blockValue>
			<fmtName>totalBugs</fmtName>
			<type>int</type>
		</field>

		<field>
			<blockValue>
				<if con="len('($23)')&gt;8">
					<expValue>(@totalBugs)+1</expValue>
				</if>
				<else>
					<value>(@totalBugs)</value>
				</else>
			</blockValue>
			<fmtName>totalBugs</fmtName>
			<type>int</type>
		</field>

		<field>
			<blockValue>
				<if con="len('($25)')&gt;8">
					<expValue>(@totalBugs)+1</expValue>
				</if>
				<else>
					<value>(@totalBugs)</value>
				</else>
			</blockValue>
			<fmtName>totalBugs</fmtName>
			<type>int</type>
		</field>



	</formatter>
	<analyser>
		<type>counter</type>
		<outputs>
			<output>
				<fmtName>highSev</fmtName>
				<expType>map</expType>
			</output>
		</outputs>
		<outputs>
			<output>
