			<timeFmt>
				<match>(.*)</match>
				<fmt>%Y/%m/%d</fmt>
			</timeFmt>
		</field>

		<field>
			<expValue>((@tacCloseDate)-(@openDate))/3600/24</expValue>
			<fmtName>tacDuration</fmtName>
			<type>int</type>
		</field>
		<field>
			<expValue>((@deCloseDate)-(@openDate))/3600/24</expValue>
